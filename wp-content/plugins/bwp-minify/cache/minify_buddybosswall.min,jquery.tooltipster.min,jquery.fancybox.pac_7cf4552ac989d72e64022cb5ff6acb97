plot.zoomOut({center:c});else
plot.zoom({center:c});}
function onMouseWheel(e,delta){e.preventDefault();return false;}
var prevCursor='default',prevPageX=0,prevPageY=0,panTimeout=null;function onDragStart(e){if(e.which!=1)
